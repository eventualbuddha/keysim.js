{
  "name": "keysim",
  "version": "1.3.0",
  "description": "Keyboard simulator for JavaScript.",
  "main": "dist/keysim.js",
  "jsnext:main": "lib/keysim.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventualbuddha/keysim.js"
  },
  "scripts": {
    "build": "gobble build dist -f",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "./node_modules/karma/bin/karma start karma.conf.js",
    "test:node": "mocha -R spec --recursive test"
  },
  "files": [
    "dist/keysim.js",
    "lib/keysim.js"
  ],
  "keywords": [
    "keyboard",
    "event",
    "dom",
    "KeyboardEvent",
    "simulate"
  ],
  "author": "Brian Donovan",
  "license": "MIT",
  "devDependencies": {
    "add-event-handler": "^1.0.0",
    "babel": "^5.8.21",
    "detect-node": "^2.0.3",
    "es5-shim": "^4.1.10",
    "esperanto": "^0.7.4",
    "gobble": "^0.10.2",
    "gobble-cli": "^0.4.2",
    "jsdom": "3.1.2",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-sauce-launcher": "^0.2.14",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "dom-element-is-natively-editable": "^1.0.3"
  }
}
