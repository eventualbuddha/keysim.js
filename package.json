{
  "name": "keysim",
  "version": "1.3.0",
  "description": "Keyboard simulator for JavaScript.",
  "main": "dist/keysim.js",
  "jsnext:main": "lib/keysim.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventualbuddha/keysim.js"
  },
  "scripts": {
    "build": "gobble build dist -f",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "./node_modules/karma/bin/karma start karma.conf.js",
    "test:node": "mocha -R spec --recursive test"
  },
  "files": [
    "dist/keysim.js",
    "lib/keysim.js"
  ],
  "keywords": [
    "keyboard",
    "event",
    "dom",
    "KeyboardEvent",
    "simulate"
  ],
  "author": "Brian Donovan",
  "license": "MIT",
  "devDependencies": {
    "add-event-handler": "^1.0.3",
    "babel": "^5.8.21",
    "browserify": "^13.0.1",
    "detect-node": "^2.0.3",
    "es5-shim": "^4.5.9",
    "esperanto": "^0.7.4",
    "gobble": "^0.10.2",
    "gobble-cli": "^0.4.2",
    "jsdom": "^3.1.2",
    "karma": "^1.1.1",
    "karma-browserify": "^5.0.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-sauce-launcher": "^1.0.0",
    "mocha": "^2.5.3",
    "watchify": "^3.7.0"
  }
}
